-- Scripts:

local function AIJHFIK_fake_script() -- LoadFrame.LocalScript 
	local script = Instance.new('LocalScript', LoadFrame)

	local LoadWn = script.Parent
	local TextLabel = LoadWn:FindFirstChild("TextLabel").l -- предполагается, что есть TextLabel внутри LoadWn
	
	local function FadeIn(object, duration)
		local steps = 20
		local waitTime = duration / steps
		for i = 1, steps do
			local transparency = 1 - (i / steps)
			object.BackgroundTransparency = transparency
			wait(waitTime)
		end
		object.BackgroundTransparency = 0
	end
	
	local function FadeOut(object, duration)
		local steps = 20
		local waitTime = duration / steps
		for i = 1, steps do
			local transparency = i / steps
			object.BackgroundTransparency = transparency
			wait(waitTime)
		end
		object.BackgroundTransparency = 1
	end
	
	local function Load()
		-- Появление
		FadeIn(LoadWn, 0.5)  -- Пример: 0.5 секунд для появления
		LoadWn.Visible = true
	
		-- Имитация загрузки с прогрессом
		for progress = 0, 100 do
			-- Обновляем прогресс в TextLabel
			if TextLabel then
				TextLabel.Text = "Loading: " .. progress .. "%"
			end
			-- Дополнительно: можно вставлять задержки, если хотите реальную задержку
			wait(0.08)
		end
	
		-- Обратный отсчёт или подтверждение завершения загрузки
		-- например, можно подождать или выполнить другие действия
	
		-- Исчезновение
		FadeOut(LoadWn, 0.5)
		LoadWn.Visible = false
	end
	
	Load()
end
coroutine.wrap(AIJHFIK_fake_script)()
